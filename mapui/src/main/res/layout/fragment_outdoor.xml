<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
			 android:layout_width="match_parent"
			 android:layout_height="match_parent"
			 >
	<com.jiahuan.svgmapview.SVGMapView
		android:id="@+id/navi_map_view"
		android:layout_width="match_parent"
		android:layout_height="match_parent">
	</com.jiahuan.svgmapview.SVGMapView>
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		>
		<AutoCompleteTextView
				android:id="@+id/main_outdoor_search"
				android:layout_width="match_parent"
				android:layout_height="@dimen/dp_42"
				android:hint="@string/outdoor_search_hint"
				android:textColor="@color/color_black"
				android:textSize="@dimen/sp_14"
				android:paddingLeft="@dimen/dp_10"
				android:layout_marginLeft="@dimen/dp_20"
				android:layout_marginRight="@dimen/dp_20"
				android:layout_marginTop="@dimen/dp_10"
				android:singleLine="true"
				android:background="@drawable/search_edit_background"
			/>
		<ImageButton
			android:id="@+id/main_outdoor_search_button"
			android:layout_width="@dimen/dp_50"
			android:layout_height="@dimen/dp_42"
			android:background="@drawable/outdoor_search"
			android:layout_marginTop="@dimen/dp_10"
			android:layout_alignParentRight="true"
			android:layout_marginRight="@dimen/dp_20"
			android:contentDescription="@string/img_all"
			/>
		<ImageView
			android:id="@+id/main_outdoor_myinfo"
			android:layout_width="@dimen/dp_60"
			android:layout_height="@dimen/dp_60"
			android:src="@drawable/outdoor_myinfo"
			android:layout_alignParentRight="true"
			android:layout_alignParentBottom="true"
			android:layout_marginRight="@dimen/dp_10"
			android:layout_marginBottom="@dimen/dp_10"
			android:contentDescription="@string/img_all"
			/>
		<LinearLayout
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:gravity="center_horizontal"
			android:orientation="vertical"
			android:layout_alignParentBottom="true"
			android:layout_marginBottom="@dimen/dp_10"
			>
			<ImageView
				android:id="@+id/main_outdoor_sos"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/main_outdoor_sos"
				android:layout_marginBottom="@dimen/dp_10"
				android:contentDescription="@string/img_all"
				/>
			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="@string/outdoor_ruler_100"
				android:textColor="@color/color_black"
				/>
			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/main_outdoor_ruler"
				android:contentDescription="@string/img_all"
				/>
		</LinearLayout>
	</RelativeLayout>
</FrameLayout>